#include <bits/stdc++.h>
using namespace std;

long long a, b, c, d, mini;

int main(){
    cin>>a>>b>>c;
    d=a*b*c;
    mini=max(mini, d);
    d=a*(b+c);
    mini=max(mini, d);
    d=(a*b)+c;
    mini=max(mini, d);
    d=a+(b*c);
    mini=max(mini, d);
    d=(a+b)*c;
    mini=max(d, mini);
    d=a+b+c;
    mini=max(mini, d);
    cout<<mini<<"\n";
}