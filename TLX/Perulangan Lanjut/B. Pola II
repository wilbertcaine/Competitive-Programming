var

        a,b,c,i:integer;

 

begin

 

        readln(a);

 

        for i:=1 to a do

        begin

 

                for b:=1 to i do

                begin

 

                        if c=10 then

                        begin

                                c:=0;

                        end;

 

                        write(c);

 

                        c:=c+1;

 

                end;

 

                writeln;

 

        end;

 

        readln;

 

end.

